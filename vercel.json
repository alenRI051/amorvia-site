{
  "version": 2,
  "builds": [
    {
      "src": "api/health/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health/index.js"
    },
    {
      "src": "/(assets|css|js|data|manifest\\.json|favicon\\.ico|robots\\.txt|sitemap\\.xml)(/.*)?",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1$2"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}